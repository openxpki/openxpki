    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "serve": "ember serve --host=0.0.0.0 --proxy=$DEV_SERVER_FORWARD_TO --secure-proxy false --output-path=dist-dev",
